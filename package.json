{
  "name": "depcheck",
  "version": "1.0.0",
  "description": "Dependency checker for .NET Core",
  "main": "dist/server.js",
  "repository": "https://github.com/mattwcole/depcheck",
  "author": "Matt Cole",
  "license": "MIT",
  "scripts": {
    "prestart": "rimraf dist",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "lint": "eslint webpack/** src/**",
    "test": "jest",
    "start:server": "webpack --config webpack/server/webpack.config.dev.babel.js",
    "start:client": "webpack-dev-server --config webpack/client/webpack.config.dev.babel.js",
    "start": "run-p start:server start:client",
    "build:server": "webpack --config webpack/server/webpack.config.prod.babel.js",
    "build:client": "webpack --config webpack/client/webpack.config.prod.babel.js -p",
    "build": "run-p build:server build:client",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "cross-env": "^5.0.5",
    "express": "^4.15.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-router-dom": "^4.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.3",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "start-server-webpack-plugin": "^2.2.0",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  }
}
